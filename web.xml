<?xml version="1.0" encoding="GBK" ?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                 http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0"
         metadata-complete="true">
<!--  <display-name>Archetype Created Web Application</display-name>-->

  <servlet>
    <servlet-name>History</servlet-name>
    <servlet-class>Servlet.HistoryServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>ClearHistory</servlet-name>
    <servlet-class>Servlet.ClearHistoryServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>GetAns</servlet-name>
    <servlet-class>Servlet.GetAnsServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Store</servlet-name>
    <servlet-class>Servlet.StoreServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Store</servlet-name>
    <url-pattern>/StoreServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>History</servlet-name>
    <url-pattern>/HistoryServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>GetAns</servlet-name>
    <url-pattern>/GetAnsServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ClearHistory</servlet-name>
    <url-pattern>/ClearHistoryServlet</url-pattern>
  </servlet-mapping>

</web-app>
